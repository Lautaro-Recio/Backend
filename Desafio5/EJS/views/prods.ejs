
    <% if(products.length===0){%>
      <h3>No hay productos</h3>
      <%}else{%>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Producto</th>
              <th scope="col">Precio</th>
              <th scope="col">Imagen</th>
            </tr>
          </thead>
          <tbody>
            <%products.forEach(prod=>{ %>
                  <tr>
                    <th><%=prod.id %></th>
                    <td><%=prod.product %></td>
                    <td><%=prod.price %></td>
                    <td><img style="height:50px; width:50px" src="<%=prod.thumbnail %>" alt=<%=prod.product %>></td>
                  </tr>
            <%})%>
              </tbody>
          </table>
      <%}%>